class Solution {
public:
    int removeDuplicates(vector<int>& num) {
        sort(num.begin(),num.end());
        int n=num.size();
        if(n<=1)return n;
        int i=0;
        for(int j=0;j<n-1;j++){
            if(num[j]!=num[j+1]){
                num[i++]=num[j];
            }
        }
       // if(num[i]!=num[n-1]){
            num[i++]=num[n-1];
        //}
        return i;
    }
};
