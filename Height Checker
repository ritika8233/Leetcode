class Solution {
public:
    int heightChecker(vector<int>& h) {
        int n=h.size();
        vector<int> v(n,0);
        for(int i=0;i<n;i++){
            v[i]=h[i];
        }
        sort(v.begin(),v.end());
        int c=0;
        for(int i=0;i<n;i++){
            if(v[i]!=h[i]){
                c++;
            }
        }
        return c;
    }
};
