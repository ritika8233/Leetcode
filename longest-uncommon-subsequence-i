class Solution {
public:
    int findLUSlength(string a, string b) {
        int n1=a.length();
        int n2=b.length();
        if(n1>n2){
            return n1;
        }
        if(n2>n1){
            return n2;
        }
        for(int i=0;i<n1;i++){
                if(a[i]!=b[i]){
                    return max(n1,n2);
                }
        }
        return -1;
    }
};
