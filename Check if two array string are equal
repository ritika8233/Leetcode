class Solution {
public:
    bool arrayStringsAreEqual(vector<string>& word1, vector<string>& word2) {
        int n1=word1.size();
        int n2=word2.size();
        if(n1==0 || n2==0)return false;
        string temp1,temp2;
        for(int i=0;i<n1;i++){
            temp1+=word1[i];
        }
        // cout<<temp1;
        for(int j=0;j<n2;j++){
            temp2+=word2[j];
        }
        if(temp1==temp2)
        return true;
        else
            return false;
    }
};
