class Solution {
public:
    vector<int> findDuplicates(vector<int>& num) {
        int n=num.size();
        vector<int> v;
        if(n<2){
            return v;
        }
        for(int i=0;i<n;i++){
            if(num[abs(num[i])-1] < 0){
                v.push_back(abs(num[i]));
            }
            num[abs(num[i])-1]=-num[abs(num[i])-1];
        }
        return v;
    }
};
