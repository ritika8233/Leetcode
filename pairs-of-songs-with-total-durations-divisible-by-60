class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& t) {
        int n=t.size();
        if(n<2)return 0;
        vector<int> v(60,0);
        int c=0;
        for(int i=0;i<n;i++){
          v[t[i]%60]++;
        }
        for(int i=1;i<30;i++){
          c+=v[i]*v[60-i];// 20(2) 40(4)
        }
        c+=(v[30]*(v[30]-1))/2;
        c+=(v[0]*(v[0]-1))/2;
        return c;
   }
};
